<table class="table table-striped">
	<thead>
		<tr>
			<th>#</th>
			<th>Имя</th>
			<th>Телефон</th>
			<th>Дата и время</th>
			<th>Цвет</th>
			<th>Парикмахер</th>
		</tr>
	</thead>
	<body>
		<% @results.each do |row| %>
			<tr>
				<td><%= row['Id']%></td>	
				<td><%= row['Name']%></td>
				<td><%= row['Phone']%> </td>
				<td><%= row['Datestamp']%> </td>
				<td><%= row['Color']%> </td>
				<td><%= row['Barber']%> </td>
			</tr>
		<% end %>
	</body>
</table>




  
  

